{
    "name": "continious-monitoring-inspection-campaign",
    "targetArn": "arn:aws:iotfleetwise:${AWS_REGION}:${AWS_ACCOUNT_ID}:fleet/blog-inspection-fleet",
    "signalCatalogArn": "arn:aws:iotfleetwise:${AWS_REGION}:${AWS_ACCOUNT_ID}:signal-catalog/main-signal-catalog",
    "collectionScheme": {
        "timeBasedCollectionScheme": {
            "periodMs": 30000
        }
    },
    "diagnosticsMode": "OFF",
    "dataExtraDimensions": [
        "VehicleVIN"
    ],
    "spoolingMode": "TO_DISK",
    "signalsToCollect": [
        {
            "name": "EVBatterySample.BMS.BatteryPack01.BatteryPackVoltagePlus",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.BatteryPackVoltageMinus",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.Drivetrain.ActualVehicleSpeed",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.Module01.Cell001.CellTemperature",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.Module02.Cell001.CellTemperature",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.Module03.Cell001.CellTemperature",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.Module04.Cell001.CellTemperature",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.ShuntPlusCurrent_a",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.ShuntMinusCurrent_a",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.ShuntPlusCurrentQualifier_enum",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.ShuntMinusCurrentQualifier_enum",
            "maxSampleCount": 3
        },
        {
            "name": "EVBatterySample.BMS.BatteryPack01.CoolingState",
            "maxSampleCount": 3
        }
    ]
}